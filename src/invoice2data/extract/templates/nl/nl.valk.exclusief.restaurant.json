            "f": 1,
            "l": 1,
            "x": 0,
            "y": 930,
            "r": 100,
            "W": 827,
            "H": 320
         },
         "regex": "T[:]\\s+(.?\\d*[(]?[0-9]?[)]?[0-9-]{7,10})"
      },
      "partner_email": {
         "parser": "regex",
         "area": {
            "f": 1,
            "l": 1,
            "x": 0,
            "y": 930,
            "r": 100,
            "W": 827,
            "H": 320
         },
