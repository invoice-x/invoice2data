            "x": 0,
            "y": 930,
            "r": 100,
            "W": 827,
            "H": 320
         },
         "regex": "[|]\\s([A-Z][a-z]+\\s?\\d{1,4}.?\\d*)\\s+[|]\\s\\d{4}\\w{2}"
